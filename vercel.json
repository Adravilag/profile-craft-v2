{
  "version": 2,
  "installCommand": "npm --prefix app/front-end install --include=dev",
  "builds": [
    {
      "src": "app/front-end/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/index.html" }]
}
