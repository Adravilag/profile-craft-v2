{
  "version": 2,
  "installCommand": "npm --prefix app/front-end ci --include=dev",
  "builds": [
    {
      "src": "app/front-end/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "outputDirectory": "app/front-end/dist",
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
