{
  "name": "profile-craft",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && cross-env NODE_ENV=production vite build",
    "build:purge": "cross-env NODE_ENV=production vite build",
    "build:prod:purge": "cross-env NODE_ENV=production vite build",
    "preview": "vite preview",
    "lint": "eslint ../../eslint.config.js . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint \"src/components/layout/Sections/Experience/**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:unit": "vitest --project unit",
    "test:sb": "vitest --project storybook",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --project unit --coverage",
    "clean": "rm -rf dist node_modules .vite",
    "purge:css": "node ./scripts/purge-css.js"
  },
